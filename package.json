{
  "name": "lambda-cluster-deploy",
  "version": "1.2.1",
  "description": "Lambda to deploy updates to EKS cluster",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "tape 'src/**/*.test.js' | tap-spec",
    "test:coverage": "nyc npm test",
    "pack": "cd src && zip -r - . -x '*.test.js' > ../$npm_package_name-$npm_package_version.zip",
    "postinstall": "cd src && npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint --fix && npm test",
      "pre-push": "npm run lint --fix && npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeAreSavvy/lambda-cluster-deploy.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/WeAreSavvy/lambda-cluster-deploy/issues"
  },
  "homepage": "https://github.com/WeAreSavvy/lambda-cluster-deploy#readme",
  "devDependencies": {
    "aws-sdk": "2.354.0",
    "aws-sdk-mock": "^4.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "nyc": "13.1.0",
    "proxyquire": "^2.1.0",
    "sinon": "7.1.1",
    "tap-spec": "5.0.0",
    "tape": "4.9.1"
  },
  "nyc": {
    "report-dir": "coverage",
    "temp-dir": "./coverage/.nyc_output",
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "**/*.test.js"
    ]
  }
}
